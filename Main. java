public class Main {
    public static void main(String[] args) {
        // Create Car object
        Car myCar = new Car(120, 40.5, 4);
        System.out.println("Car:");
        myCar.start();
        myCar.displayFuel();
        myCar.honk();
        System.out.println("Doors: " + myCar.getNumberOfDoors());
        System.out.println();

        // Create Bicycle object
        Bicycle myBike = new Bicycle(20, 0.0, true);
        System.out.println("Bicycle:");
        myBike.start();
        myBike.displayFuel();
        myBike.ringBell();
        System.out.println("Has Bell: " + myBike.getHasBell());
    }
}
